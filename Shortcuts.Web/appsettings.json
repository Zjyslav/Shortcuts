{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Grpc": {
    "Shortcuts": "http://localhost:5000"
  },
  "ShortcutProfiles": {
    "Krita": {
      "ProcessName": "krita",
      "Shortcuts": {
        "Undo": "^(z)",
        "Redo": "^+(z)",
        "FullScreen": "{Tab}",
        "ClearLayer": "{BACKSPACE}",
        "Erase": "e"
      }
    }
  }
}
