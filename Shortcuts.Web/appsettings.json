{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "Grpc": {
    "Shortcuts": "http://localhost:5000"
  },
  "ShortcutProfiles": {
    "Krita": {
      "ProcessName": "krita",
      "Shortcuts": {
        "Undo": "^(z)",
        "Redo": "^+(z)",
        "FullScreen": "{Tab}",
        "ClearLayer": "{BACKSPACE}",
        "Erase": "e"
      }
    },
    "Aseprite": {
      "ProcessName": "Aseprite",
      "Shortcuts": {
        "Undo": "^(z)",
        "Redo": "^+(z)",
        "PencilTool": "b",
        "Eraser": "e",
        "Eyedropper": "i",
        "BucketTool": "g",
        "AdvancedMode": "^(f)",
        "Grid": "^(')",
        "FullscreenPreview": "{F8}",
        "FitOnScreen": "^(0)",
        "Clear": "{BACKSPACE}"
      }
    }
  }
}
